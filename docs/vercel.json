{
  "devCommand": "uvicorn src.documentation:app --reload --reload-dir .",
  "builds": [
    {
      "src": "src/documentation/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/documentation/main.py"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=600, stale-while-revalidate=30"
        }
      ]
    }
  ]
}
